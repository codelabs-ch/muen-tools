 <expressions>
  <expression name="session2_enabled">
   <gt>
    <variable name="session_count"/>
    <integer value="1"/>
   </gt>
  </expression>
  <expression name="session2_disabled">
   <gt>
    <integer value="2"/>
    <variable name="session_count"/>
   </gt>
  </expression>
  <expression name="int_values">
   <gt>
    <integer value="5"/>
    <integer value="1"/>
   </gt>
  </expression>
  <expression name="int_values_2">
   <lt>
    <integer value="42"/>
    <integer value="23"/>
   </lt>
  </expression>
  <expression name="is_below_max">
   <lt>
    <variable name="session_count"/>
    <integer value="255"/>
   </lt>
  </expression>
  <expression name="has_four_sessions">
   <eq>
    <variable name="session_count"/>
    <integer value="4"/>
   </eq>
  </expression>
  <expression name="two_is_two">
   <eq>
    <integer value="2"/>
    <integer value="2"/>
   </eq>
  </expression>
  <expression name="two_is_three">
   <eq>
    <integer value="2"/>
    <integer value="3"/>
   </eq>
  </expression>
  <expression name="has_not_three_sessions">
   <ne>
    <variable name="session_count"/>
    <integer value="3"/>
   </ne>
  </expression>
  <expression name="two_is_not_three">
   <ne>
    <integer value="2"/>
    <integer value="3"/>
   </ne>
  </expression>
  <expression name="two_is_not_two">
   <ne>
    <integer value="2"/>
    <integer value="2"/>
   </ne>
  </expression>
  <expression name="iommu_disabled">
   <not>
    <variable name="feature_enabled"/>
   </not>
  </expression>
  <expression name="not_false">
   <not>
    <boolean value="false"/>
   </not>
  </expression>
  <expression name="and_expr">
   <and>
    <boolean value="false"/>
    <variable name="feature_enabled"/>
   </and>
  </expression>
  <expression name="and_expr_2">
   <and>
    <boolean value="true"/>
    <boolean value="true"/>
   </and>
  </expression>
  <expression name="or_expr">
   <or>
    <boolean value="false"/>
    <variable name="feature_enabled"/>
   </or>
  </expression>
  <expression name="or_expr_2">
   <or>
    <boolean value="false"/>
    <boolean value="false"/>
   </or>
  </expression>
  <expression name="nested_expr">
   <and>
    <expression name="session2_enabled">
     <gt>
      <variable name="session_count"/>
      <integer value="1"/>
     </gt>
    </expression>
    <expression name="sub_expression">
     <or>
      <expression name="sub_sub_expression">
       <not>
        <boolean value="true"/>
       </not>
      </expression>
      <variable name="feature_enabled"/>
     </or>
    </expression>
   </and>
  </expression>
 </expressions>
