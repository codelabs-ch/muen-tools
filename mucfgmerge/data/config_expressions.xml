   </gt>
  </expression>
  <expression name="session2_disabled">
   <gt>
    <integer value="2"/>
    <variable name="session_count"/>
   </gt>
  </expression>
  <expression name="int_values">
   <gt>
    <integer value="5"/>
    <integer value="1"/>
   </gt>
  </expression>
  <expression name="int_values_2">
   <lt>
    <integer value="42"/>
    <integer value="23"/>
   </lt>
  </expression>
  <expression name="is_below_max">
   <lt>
    <variable name="session_count"/>
    <integer value="255"/>
   </lt>
  </expression>
  <expression name="has_four_sessions">
   <eq>
    <variable name="session_count"/>
    <integer value="4"/>
   </eq>
  </expression>
  <expression name="two_is_two">
   <eq>
    <integer value="2"/>
    <integer value="2"/>
   </eq>
  </expression>
  <expression name="two_is_three">
   <eq>
    <integer value="2"/>
    <integer value="3"/>
   </eq>
  </expression>
  <expression name="has_not_three_sessions">
   <ne>
    <variable name="session_count"/>
    <integer value="3"/>
   </ne>
  </expression>
  <expression name="two_is_not_three">
   <ne>
    <integer value="2"/>
    <integer value="3"/>
   </ne>
  </expression>
  <expression name="two_is_not_two">
   <ne>
    <integer value="2"/>
    <integer value="2"/>
   </ne>
  </expression>
  <expression name="truth">
   <boolean value="true"/>
  </expression>
  <expression name="iommu_on">
   <variable name="iommu_enabled"/>
  </expression>
  <expression name="iommu_disabled">
   <not>
    <variable name="iommu_enabled"/>
   </not>
  </expression>
  <expression name="true">
   <not>
    <boolean value="false"/>
   </not>
  </expression>
 </expressions>
