with SK;

package Skp.IOMMU
is

   Root_Table_Address : constant := 16#0030_0000#;
   Base_Address       : constant := 0;

   type IOMMU_Device_Range is range 1 .. 0;

   type IOMMU_Device_Array is array (IOMMU_Device_Range) of SK.Word64;

   IOMMUs : constant IOMMU_Device_Array := IOMMU_Device_Array'(
       others => 0);

end Skp.IOMMU;
