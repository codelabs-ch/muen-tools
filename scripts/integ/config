### muen_integ_config ###

ROOT=$HOME/Builds
LOGDIR=$ROOT/logs/$BUILDID
LOGFILE=$LOGDIR/build.log
LOCK=/tmp/muen-integ.running

# AMT password, only set if not present in the environment
: ${AMT_PASSWORD=XXX}

# mail settings
RECIPIENTS="postmaster@localhost"
