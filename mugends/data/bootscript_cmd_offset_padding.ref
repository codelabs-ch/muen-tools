echo "Loading files from TFTP"
tftpboot 0x100000 kernel.bin-kernel_text.part || panic
tftpboot 0x170000 kernel.bin-kernel_ro.part || panic
tftpboot 0x182000 kernel.bin-kernel_data_0.part || panic
mw 0x184000 0x0 0x2000
tftpboot 0x186000 kernel.bin-kernel_global_data.part || panic
tftpboot 0x1a0000 kernel.bin-kernel_stack.part || panic
mw 0x1c0000 0x0 0x10000

dcache flush
cpu 1 release 0x100000
go 0x100000
echo "Failed to boot"
sleep 1000

